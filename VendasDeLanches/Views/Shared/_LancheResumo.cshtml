@model Lanche 


<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="img-thumbnail">
        <img src="@Model.ImagemUrl" />
    </div>

    <div class="caption">
        <h4> Lanche : @Model.Nome  </h4>
        <p>@Model.Preco.ToString("c")</p>
    </div>

    <p>@Model.DescricaoCurta</p>
</div>
